import numpy as np
import pandas as pd

data_mahasiswa = pd.read_csv("dataMahasiswa.csv")
data_mahasiswa

#data ipk
data_ipk = np.array(data_mahasiswa)
data_ipk = data_ipk[:,[0,1]]

ipk_A = np.sum(data_ipk[0:1,1], axix=0)
ipk_B = np.sum(data_ipk[0:1,1], axix=0)

print("ipk_A : ", ipk_A)
print("ipk_B : ", ipk_B)
